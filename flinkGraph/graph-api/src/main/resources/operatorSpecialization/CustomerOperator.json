{
	"title": "CustomerOperator",
	"description": "这是一个自定义sql节点",
	"type": "object",
	"format": "categories",
	"basicCategoryTitle": "service",
	"properties": {
		"service": {
			"properties": {
				"statementBody": {
					"type": "string",
					"description": "field for statementBody",
					"format": "autoinput",
							"autoinput": {
								"name": "my-autoComplete",
								"function": {
									"name": "getFieldsByConfig"
								}
							}
				}
			}
		},
		"output": {
			"type": "object",
			"properties": {
				"columns": {
					"type": "array",
					"title": "mapping",
					"items": {
						"type": "object",
						"title": "columns",
						"properties": {
							"name": {
								"type": "string",
								"pattern": "^[A-Za-z0-9_]*$",
								"title": "outName"
							},
							"type": {
								"type": "string",
								"minLength": 1,
								"title": "type",
								"default": "STRING",
								"enum": [
									"CHAR",
									"VARCHAR",
									"STRING",
									"BOOLEAN",
									"BINARY",
									"VARBINARY",
									"BYTES",
									"DECIMAL",
									"TINYINT",
									"SMALLINT",
									"INT",
									"BIGINT",
									"FLOAT",
									"DOUBLE",
									"DATE",
									"TIME",
									"TIMESTAMP",
									"TIMESTAMP(3)",
									"TIMESTAMP_LTZ(3)",
									"INTERVAL",
									"ARRAY",
									"MULTISET",
									"MAP",
									"ROW",
									"RAW"
								],
								"examples": [
									"STRING",
									"INT",
									"TIMESTAMP(3)"
								]
							},
							"desc": {
								"type": "string",
								"description": "备注"
							}
						}
					}
				}
			}
		}
	}
}