{
    "type": "object",
    "title": "MySQLSinkOperator",
	"minItems": 1,
        "required": [
			"tableName",
			"connector",
			"url",
			"username",
			"password"
        ],
        "properties": {

		"tableName": {
			"type": "string",
			"minLength": 1,
			"title": "tableName",
			"description": "mysql data sink table name"
		},
		"connector": {
			"type": "string",
			"default": "jdbc",
			"minLength": 1,
			"title": "connector",
			"enum":["jdbc"]
		},
		"url": {
			"type": "string",
			"minLength": 1,
			"title": "jdbc-url",
			"description": "eg: jdbc:mysql://127.0.0.1:3306",
			"examples": [
				"jdbc:mysql://127.0.0.1:3306"
			]
		},
		"username": {
			"type": "string",
			"default": "root",
			"minLength": 1,
			"title": "username",
			"examples": [
				"root"
			]
		},
		"password": {
			"type": "string",
			"default": "123456",
			"minLength": 1,
			"title": "password",
			"examples": [
				"123456"
			]
		},
		
	   "other":{
		 "title": "otherConnect",
		  "type": "array",
		  "items": {
			"type": "object",
			"title": "otherConfig",
			"required": [
				"key",
				"values"
			],
			"properties": {
				"key": {
					"type": "string",
					"minLength": 1,
					"description": "custom field name    eg: hostName port ...",
					"title": "key"
				},
				"values": {
					"type": "string",
					"minLength": 1,
					"description": "custom field name   eg: 225.0.0.1  8000 ...",
					"title": "values"
				}
			}
		  }
	   },
		
		"columns": {
			"type": "array",
			"minItems": 1,
			"title": "columns",
			"items": {
				"type": "object",
				"title": "schema",
				"required": [
					"name",
					"type",
					"flag"
				],
				"properties": {
					"name": {
						"type": "string",
						"minLength": 1,
						"title": "name",
						"examples": [
							"typ",
							"taskId",
							"taskStatus",
							"dt",
							"WATERMARK FOR dt AS dt - INTERVAL '15' SECOND",
							"PRIMARY KEY(taskId) NOT ENFORCED "
						],
						"description": "field name"
					},
					"type": {
						"type": "string",
						"minLength": 1,
						"title": "type",
						"default": "STRING",
						"enum":["CHAR","VARCHAR","STRING","BOOLEAN","BINARY","VARBINARY","BYTES","DECIMAL","TINYINT","SMALLINT","INTEGER","BIGINT","FLOAT","DOUBLE","DATE","TIME","TIMESTAMP","TIMESTAMP_LTZ","INTERVAL","ARRAY","MULTISET","MAP","ROW","RAW"],
						"examples": [
							"STRING",
							"INT",
							"TIMESTAMP(3)"
						]
					},
					"flag":{
						"type": "boolean",
						"default": true,
						"title": "checked ?"
					}
				}
			}
		}
	}
}